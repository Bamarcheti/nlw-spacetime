{
    "extends": [
        "next/core-web-vitals",
        "@rocketseat/eslint-config/node"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "semi": [
            "error",
            "always"
        ],
    }
}